    },
    "devDependencies": {
        "@replyguy/config": "workspace:*",
        "@types/dotenv": "^8.2.3",
        "@types/node": "^20.10.0",
        "dotenv": "^16.5.0",
        "eslint": "^9.28.0",
        "jest": "^29.7.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.0"
    }
