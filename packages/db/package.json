{
    "name": "@replyguy/db",
    "version": "0.0.0",
    "private": true,
    "description": "Supabase client with typed queries for ReplyGuy",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.build.json --noEmit false",
