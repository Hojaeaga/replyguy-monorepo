{
  "name": "@replyguy/user",
  "version": "1.0.0",
  "private": true,
  "description": "User management utilities for ReplyGuy",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json --noEmit false",
    "dev": "tsc -p tsconfig.build.json --watch --noEmit false",
    "check-types": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@replyguy/db": "workspace:*",
    "@replyguy/neynar": "workspace:*",
    "@replyguy/ai_service": "workspace:*"
  },
  "devDependencies": {
    "@replyguy/config": "workspace:*",
    "eslint": "^9.28.0",
    "typescript": "^5.0.0"
  }
}